

//1)Using for setting values


public class Cars {

	public String model;
	public String doors;


	public static final String FirstCarModel = "Toyota";
	public static final String FirstCarDoors = "4";


	public static final String SecondCarModel = "Mazda";
	public static final String SecondCarDoors = "2";


	public Cars withModel(String model) {
		this.model = model;
		return this;
	}

	public Cars withDoors(String doors) {
		this.doors = doors;
		return this;
	}
}

//3) Using for setting tests with hardcoded values


public class ChainCaller {

    @Test
	public void setCarValues1() {

        //Setting values
		Cars car = new Cars().withModel(Cars.FirstCarModel).withDoors(Cars.FirstCarDoors);

        //Method execution
		settingValues(car);

	}

	    @Test
    	public void setCarValues2() {

        //Setting values
        Cars car = new Cars().withModel(Cars.SecondCarModel).withDoors(Cars.SecondCarDoors);

        //Method execution
    	settingValues(car);

    	}
}



public class Car {
		public String iModel;
		public static int iWheels;
		public static int iDoors;

	public Car(){
		iWheels = 4;
		}

	public void DisplayCharacterstics() {
        System.out.println(iModel);
        System.out.println(iWheels);
        System.out.println(iDoors);
        }

    public static void DisplayStaticCharacterstics() {
        System.out.println(iWheel);
        System.out.println(iDoors);
        }
	}


public class TestStaticModifiers

	public static void main(String[] args) {
        Car Toyota = new Car();
        Toyota.iModel="Camry";
        Car.iDoors = 2;

        System.out.println(Toyota.iModel);
        System.out.println(Car.iWheel);
        System.out.println(Car.iDoors);

        Car.DisplayStaticCharacterstics();
        Toyota.DisplayCharacterstics();

        }
    }